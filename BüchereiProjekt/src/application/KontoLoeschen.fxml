<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.KontoLoeschenController">
   <children>
      <AnchorPane fx:id="Hinweisflaeche" layoutX="-10.0" layoutY="-7.0" prefHeight="200.0" prefWidth="400.0" style="-fx-background-color: #1d1d1b;">
         <children>
            <Label fx:id="labelHinweis" layoutX="114.0" layoutY="165.0" prefHeight="16.0" prefWidth="210.0" textFill="RED">
               <font>
                  <Font name="Franklin Gothic Medium" size="14.0" />
               </font>
            </Label>
            <Button fx:id="buttonOk" layoutX="174.0" layoutY="120.0" mnemonicParsing="false" onAction="#handleButtonOkAction" style="-fx-background-color: #ff9900;" text="OK!" textFill="WHITE">
               <font>
                  <Font name="Franklin Gothic Medium" size="18.0" />
               </font>
            </Button>
            <Button fx:id="buttonBack" layoutX="22.0" layoutY="160.0" mnemonicParsing="false" onAction="#handleButtonBackAction" style="-fx-background-color: #4d4e4f;" text="back" textFill="WHITE">
               <font>
                  <Font name="Franklin Gothic Medium" size="14.0" />
               </font>
            </Button>
            <TextField fx:id="tfEmailL" layoutX="90.0" layoutY="51.0" prefHeight="26.0" prefWidth="234.0" />
            <TextField fx:id="tfPasswortL" layoutX="90.0" layoutY="87.0" prefHeight="26.0" prefWidth="234.0" />
            <Label layoutX="22.0" layoutY="54.0" text="Email:" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label layoutX="22.0" layoutY="90.0" text="Passwort:" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
