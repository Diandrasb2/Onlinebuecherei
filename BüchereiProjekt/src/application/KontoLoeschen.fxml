<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.KontoLoeschenController">
   <children>
      <AnchorPane fx:id="Hinweisflaeche" layoutX="-10.0" layoutY="-7.0" prefHeight="200.0" prefWidth="400.0" style="-fx-background-color: #1d1d1b;">
         <children>
            <Label fx:id="labelHinweis" layoutX="83.0" layoutY="67.0" text="Willst du dein Konto entgültig löschen?" textFill="WHITE">
               <font>
                  <Font name="Franklin Gothic Medium" size="14.0" />
               </font>
            </Label>
            <Button fx:id="buttonOk" layoutX="174.0" layoutY="100.0" mnemonicParsing="false" onAction="#handleButtonOkAction" style="-fx-background-color: #ff9900;" text="OK!" textFill="WHITE">
               <font>
                  <Font name="Franklin Gothic Medium" size="18.0" />
               </font>
            </Button>
            <Button fx:id="buttonBack" layoutX="22.0" layoutY="160.0" mnemonicParsing="false" onAction="#handleButtonBackAction" style="-fx-background-color: #4d4e4f;" text="back" textFill="WHITE">
               <font>
                  <Font name="Franklin Gothic Medium" size="14.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
